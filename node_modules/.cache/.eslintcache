-items: center;\ntransition: all 1s ease;\n\n&>:first-child{\n    animation-name: ${upAndDown};\n    animation-duration: 0.7s;\n    animation-direction: alternate;\n    animation-timing-function: cubic-bezier(0.95, 0.05, 0.795, 0.035);\n    animation-iteration-count: infinite;\n }\n\n&>:last-child{\n    display: ${props => props.click ? 'none' : 'inline-block'};\n    padding-top: 10rem;\n}\n\n`\n\nconst DarkDiv = styled.div`\nposition: absolute;\nbackground-color: #000;\ntop: 0;\nbottom: 0;\nright: 50%;\nwidth: ${props => props.click ? '50%' : '0%'};\nheight: ${props => props.click ? '100%' : '0%'};\nz-index:1;\ntransition: height 0.5s ease, width 1s ease 0.5s;\n`\n\n\nconst Intro = () => {\n\n    const [click, setClick] = useState(false);\n    const handleClick = () => {\n        setClick(!click)\n    }\n\n    const changeClick = () => {\n        if (window.scrollY >= \"30\") {\n            setClick(true);\n        }\n    }\n\n\n    window.addEventListener('scroll', changeClick);\n\n    return (\n        <IntroContainer id=\"home\">\n            <DarkDiv click={click} />\n            <Container>\n                <Socials theme={click ? 'dark' : 'light'} />\n                <Navbar />\n                <Center click={click} onClick={() => handleClick()}>\n                    <Basketball width={120} height={120} fill='currentColor' />\n                    <span>click me</span>\n                </Center>\n            </Container>\n            {click ? <IntroContent click={click} /> : null}\n        </IntroContainer>\n    )\n}\n\nexport default Intro; ","/Users/Gurjo/Dev/javascript/react/myportfolio/src/components/About.js",[],"/Users/Gurjo/Dev/javascript/react/myportfolio/src/components/Work.js",[],"/Users/Gurjo/Dev/javascript/react/myportfolio/src/components/Projects.js",[],"/Users/Gurjo/Dev/javascript/react/myportfolio/src/subComponents/Socials.js",["72"],"import React from \"react\";\nimport styled from \"styled-components\";\nimport { NavLink } from \"react-router-