ack\"} style={{ textDecoration: 'none', color: 'inherit', marginBottom: \"0.8rem\" }} >\n                <motion.h4\n                    whileHover={{ scale: 1.1 }}\n                    whileTap={{ scale: 0.9 }}\n                >// work</motion.h4>\n            </a>\n            <a href=\"#projects\" className={color4 ? \"navbar-white\" : \"navbar-black\"} style={{ textDecoration: 'none', color: 'inherit' }}>\n                <motion.h4\n                    whileHover={{ scale: 1.1 }}\n                    whileTap={{ scale: 0.9 }}\n                >// projects</motion.h4>\n            </a>\n\n        </Pages>\n\n    )\n}\n\nexport default Navbar","/Users/Gurjo/Dev/javascript/react/myportfolio/src/components/IntroContent.js",[],"/Users/Gurjo/Dev/javascript/react/myportfolio/src/subComponents/Particle.js",[],"/Users/Gurjo/Dev/javascript/react/myportfolio/src/subComponents/WorkData.js",[],["77","78"],"/Users/Gurjo/Dev/javascript/react/myportfolio/src/components/TabPanel.js",["79"],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":8,"column":10,"nodeType":"86","messageId":"87","endLine":8,"endColumn":16},{"ruleId":"84","severity":1,"message":"88","line":42,"column":7,"nodeType":"86","messageId":"87","endLine":42,"endColumn":13},{"ruleId":"84","severity":1,"message":"89","line":6,"column":10,"nodeType":"86","messageId":"87","endLine":6,"endColumn":18},{"ruleId":"90","severity":1,"message":"91","line":67,"column":18,"nodeType":"92","messageId":"93","endLine":67,"endColumn":25},{"ruleId":"90","severity":1,"message":"91","line":73,"column":18,"nodeType":"92","messageId":"93","endLine":73,"endColumn":29},{"ruleId":"90","severity":1,"message":"91","line":79,"column":18,"nodeType":"92","messageId":"93","endLine":79,"endColumn":25},{"ruleId":"90","severity":1,"message":"91","line":85,"column":18,"nodeType":"92","messageId":"93","endLine":85,"endColumn":29},{"ruleId":"80","replacedBy":"94"},{"ruleId":"82","repl